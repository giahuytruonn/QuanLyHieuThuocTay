	<parameter name="maHoaDonParam" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT 
    "HoaDon"."maHoaDon",
    "HoaDon"."ngayTao",
    "HoaDon"."tongGiaTriHoaDon",
    "HoaDon"."tongTienKhachTra",
    "HoaDon"."tienDaGiam",
    "HoaDon"."tongTienThanhToan",
    "HoaDon"."maChietKhau",
    "HoaDon"."tongTienTraLai",
    "HoaDon"."phuongThucThanhToan",
    "SanPham"."tenSanPham",
    "ChiTietHoaDon"."tongTien",
    "ChiTietHoaDon"."soLuong",
	"ChiTietHoaDon"."tongTien"/"ChiTietHoaDon"."soLuong" as N'Đơn giá',
    ISNULL("KhachHang"."hoTen", '') AS tenKhachHang,
    ISNULL("KhachHang"."email", '') AS emailKhachHang,
    "NhanVien"."tenNhanVien",
    CASE
        WHEN "HoaDon"."phuongThucThanhToan" = 0 THEN N'TIỀN MẶT'
        ELSE 'QR CODE'
    END AS phuongThucThanhToanKH
FROM 
    "ChiTietHoaDon"
    INNER JOIN "HoaDon" ON "HoaDon"."maHoaDon" = "ChiTietHoaDon"."maHoaDon"
    INNER JOIN "SanPham"  ON "SanPham"."maSanPham" = "ChiTietHoaDon"."maSanPham"
    LEFT JOIN "KhachHang" ON "HoaDon"."maKhachHang" = "KhachHang"."maKhachHang"
    LEFT JOIN "NhanVien" ON "HoaDon"."maNhanVien" = "NhanVien"."maNhanVien"
WHERE 
    "HoaDon"."maHoaDon" =  $P{maHoaDonParam} 
GROUP BY 
    "HoaDon"."maHoaDon",
    "HoaDon"."ngayTao",
    "HoaDon"."tongGiaTriHoaDon",
    "HoaDon"."tongTienKhachTra",
    "HoaDon"."tienDaGiam",
    "HoaDon"."tongTienThanhToan",
    "HoaDon"."maChietKhau",
    "HoaDon"."tongTienTraLai",
    "HoaDon"."phuongThucThanhToan",
    "KhachHang"."hoTen",
    "KhachHang"."email",
    "SanPham"."tenSanPham",
    "ChiTietHoaDon"."tongTien",
    "ChiTietHoaDon"."soLuong",
    "ChiTietHoaDon"."tongTien"/"ChiTietHoaDon"."soLuong" ,
    "NhanVien"."tenNhanVien";]]>
	</queryString>
